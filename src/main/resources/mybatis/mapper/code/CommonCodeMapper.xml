<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="my.domain.code.CommonCodeMapper">

    <resultMap id="commonCodeResultMap" type="my.domain.code.CommonCodeVO">
        <result property="groupCode" column="GROUP_CODE"/>
        <result property="code" column="CODE"/>
        <result property="codeName" column="CODE_NAME"/>
    </resultMap>

    <select id="selectByGroupCodeAndCodeName" resultMap="commonCodeResultMap">
        SELECT * FROM COMMON_CODE
        WHERE GROUP_CODE = #{groupCode} AND CODE_NAME = #{codeName}
    </select>

</mapper>
